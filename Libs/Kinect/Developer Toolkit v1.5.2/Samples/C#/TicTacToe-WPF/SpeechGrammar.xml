<grammar version="1.0" xml:lang="en-US" tag-format="semantics/1.0-literals" xmlns="http://www.w3.org/2001/06/grammar">
  <rule id="welcomeRule" scope="public">
    <one-of>
      <item>
        <tag>START</tag>
        <one-of>
          <item>start</item>
          <item>start game</item>
        </one-of>
      </item>
    </one-of>
  </rule>
  <rule id="gameRule" scope="public">
    <one-of>
      <item>
        <tag>RULESENABLE</tag>
        <one-of>
          <item>enable rules</item>
        </one-of>
      </item>
      <item>
        <tag>RULESDISABLE</tag>
        <one-of>
          <item>disable rules</item>
        </one-of>
      </item>
      <item>
        <tag>1</tag>
        <one-of>
          <item>one</item>
        </one-of>
      </item>
      <item>
        <tag>2</tag>
        <one-of>
          <item>two</item>
        </one-of>
      </item>
      <item>
        <tag>3</tag>
        <one-of>
          <item>three</item>
        </one-of>
      </item>
      <item>
        <tag>4</tag>
        <one-of>
          <item>four</item>
        </one-of>
      </item>
      <item>
        <tag>5</tag>
        <one-of>
          <item>five</item>
        </one-of>
      </item>
      <item>
        <tag>6</tag>
        <one-of>
          <item>six</item>
        </one-of>
      </item>
      <item>
        <tag>7</tag>
        <one-of>
          <item>seven</item>
        </one-of>
      </item>
      <item>
        <tag>8</tag>
        <one-of>
          <item>eight</item>
        </one-of>
      </item>
      <item>
        <tag>9</tag>
        <one-of>
          <item>nine</item>
        </one-of>
      </item>
    </one-of>
  </rule>
  <rule id="gameEndRule" scope="public">
    <one-of>
      <item>
        <tag>REMATCH</tag>
        <one-of>
          <item>rematch</item>
          <item>new game</item>
        </one-of>
      </item>
    </one-of>
  </rule>
</grammar>